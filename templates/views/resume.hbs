<div id="wrapperRow">
    <div class="wrapper-mid">
        <!-- Begin Paper -->
        <div id="paper">
            <div id="paper-mid">
                <div class="entry">
                    <div class="left_penal"><h1>{{data.employee.Name}}</h1>
                        <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. </p></div>
                    <ul class="right_penal top_ul">
                        <li class="ad">{{data.employee.address}}</li>
                        <li class="mail">{{data.employee.city}} {{data.employee.zipCode}} 
                        </li>
                        <li class="tel">{{data.employee.mobile}}
                        </li>
                        <li class="web">{{data.employee.email}}
                        </li>
                    </ul>
                </div>
                <div class="entry-row">
                    <div class="left_penal">
                        <div class="details_content">
                            <h2>EXPERIENCE</h2>
							{{#each data.experience}}
								<span class="location">{{company}} — {{designation}}</span>
								<span class="month_present">{{convertDateFormat start}}-{{convertDateFormat end}}</span>
								<p>{{description}} </p>
							{{/each}}	
                        </div>
                        
						{{#each data.projects}}
							<div class="details_content">
								<h2>PROJECTS</h2>
								<span class="location">{{project.name}} — {{project.url}}</span>
								<span class="month_present">MONTH 20XX - PRESENT</span>
								<p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. </p>
							</div>
						{{/each}}	

                    </div>
                    <div class="right_penal">
							<div class="right_penal_details">
								<h2>SKILLS</h2>
								{{#each data.skills}}
									<p>{{skill.name}}</p>
								{{/each}}	
							</div>
                        <div class="right_penal_details">
                            <h2>AWARDS</h2>
                            <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. </p>
                        </div>
                        <div class="right_penal_details">
                            <h2>LANGUAGES</h2>
                            <ul class="language">
                                <li>Hindi</li>
                                <li>English</li>
                                <li>Tamil</li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<style>
    body {
        font-family: 'Open Sans', sans-serif;
        background: #dedede;
    }
    #wrapperRow {
        width: 969px;
        margin: 0 auto;
    }

    #paper {
        width: 828px;
        margin: 0 auto;
    }


    #paper-mid {
        width: 828px;
        margin: 0 auto;
        background-color: #fff;
        display: block;
        padding:  50px;
        overflow: hidden;
    }

    .entry {
        display: flex;
        clear: both;
        padding-bottom: 40px;
    }
    .entry-row {
        display: flex;
        margin-bottom: 30px;
    }
    .right_penal {
        flex: 0 0 25%;
    }
    .left_penal {
        flex: 0 0 75%;
    }
    h1 {
        font-size: 42px;
        color: #51383b;
        margin-bottom: 3px;
        clear: both;
        margin-top: 6px;
        display: block;
    }

    h2 {
        margin: 0;
        margin-bottom: 10px;
        clear: both;
        font-size: 18px;
        color: #1a73e8;
        font-weight: 400;
    }

    p {
        font-size: 14px;
        color: #6b6868;
        margin: 5px 0 0;
    }


    h3 {
        width: 180px;
        text-align: right;
        float: left;
        padding: 0;
        clear: both;
        font-size: 13px;
        color: #7d6a6c;
    }

    ul.top_ul {
        display: block;
        padding: 0;
    }
    ul.top_ul li{
        list-style: none;
        font-size: 15px;
        font-weight: 400;
        line-height: 23px;
        display: block;
    }
    .location {
        display: block;
        font-size: 20px;
        font-weight: 600;
        color: #000;
    }
    .month_present {
        display: block;
        font-size: 15px;
        font-weight: 400;
        margin-top: 5px;
        text-transform: uppercase;
        color: #6b6868;
    }
    .details_content {
        display: block;
        margin-bottom: 30px;
    }
    .right_penal_details {
        display: block;
        margin-bottom: 30px;
    }
    ul.language{
        margin: 0;
        padding: 0;
        display: block;
    }
    ul.language li{
        padding: 0;
        margin-left: 20px;
        list-style: circle;
        font-size: 16px;
        font-weight: 400;
    }
</style>
