{{>header}}
<div class="main-wrapper">
	<section id="home-section" class="hero">
		<div class="container content">
			<div class=" slick-slider-section">
				<div class="">
					<div class="item">
						<div class="row-col">
							<div class="content-col">
								<span class="subheading">Hello!</span>
								<h1 class="mb-4 mt-3">I'm a <span>{{data.title}}</span> based in {{data.city}}</h1>
								<p><a href="" class="btn btn-primary py-3 px-4">Hire me</a> <a href=""
																								class="btn btn-white btn-outline-white py-3 px-4">My
									works</a></p>
							</div>
                            <div class="heroSlider">
						{{#each data.userImages}}
							<div class="img-col">
								<img
									src="{{{cloudinaryUrl this}}}"/>
							</div>
						{{/each}}
						</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>
	<section class="about-section default-section pb-0" id="about-section">
		<div class="container">
			<div class="row">
				<div class="col-md-6 col-lg-5 d-flex">
					<div class="img-about img d-flex align-items-stretch">
						<div class="overlay"></div>
						{{#if data.image}}
							<div class="img d-flex align-self-stretch align-items-center"
								 style="background-image:url({{{cloudinaryUrl data.image}}});">
							</div>
						{{/if}}
					</div>
				</div>
				<div class="col-md-6 col-lg-7 pl-lg-5 pb-5">
					<div class="row justify-content-start pb-3">
						<div class="col-md-12 heading-section ftco-animate fadeInUp ftco-animated">
							<h1 class="big">About</h1>
							<h2 class="mb-4 text-white">About Me</h2>
							<p>{{data.aim}}</p>
							<ul class="about-info mt-4 px-md-0 px-2">
								{{#each data.employee}}
									<li class="d-flex"><span>{{@key}}:</span> <span>{{this}}</span></li>
								{{/each}}
							</ul>
						</div>
					</div>
					<div class="counter-wrap ftco-animate d-flex mt-md-3 fadeInUp ftco-animated">
						<div class="text">
							<p class="mb-4">
								<span class="number" data-number="120">{{data.projects.length}}</span>
								<span>{{projectCount data.projects.length}}</span>
							</p>
							<form action="/resume/{{{data.slug}}}" target="_blank" method="get">
							<p><button type="submit" class="btn btn-primary py-3 px-3" >Download CV</button></p>
                            </form>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!--	EndAbout-->
	<section class="default-section pb-0" id="resume-section">
		<div class="container">
			<div class="row justify-content-center pb-5">
				<div class="col-md-10 heading-section text-center ftco-animate fadeInUp ftco-animated">
					<h1 class="big right-0">Resume</h1>
					<h2 class="mb-4 text-white">Resume</h2>
					<p>{{data.aim}}</p>
				</div>
			</div>
			<div class="row">
			{{#each data.experience}}
				<div class="col-md-6">
					<div class="resume-grid ftco-animate fadeInUp ftco-animated">
						<span class="date">{{convertDateFormat start}}-{{convertDateFormat end}}</span>
						<h2>{{company}}</h2>
						<span class="position">{{designation}}</span>
						<p class="mt-4">{{description}}</p>
					</div>
				</div>
				{{/each}}
			</div>
		</div>
	</section>
	<!--	EndResume-->
	<section class="default-section pb-0" id="skills-section">
		<div class="container">
			<div class="row justify-content-center pb-5">
				<div class="col-md-12 heading-section text-center ftco-animate fadeInUp ftco-animated">
					<h1 class="big right-0">Skills</h1>
					<h2 class="mb-4 text-white">My Skills</h2>
					<p>Far far away, behind the word mountains, far from the countries Vokalia and Consonantia</p>
				</div>
			</div>
			<div class="row">
			{{#each data.skills}}
				<div class="col-md-6 animate-box">
					<div class="progress-row ftco-animate fadeInUp ftco-animated">
						<h3>{{skill.name}}</h3>
						<div class="progress">
							<div class="progress-bar color-1" role="progressbar" aria-valuenow="90" aria-valuemin="0"
								 aria-valuemax="100" style="width:{{rating}}%">
								<span>{{rating}}%</span>
							</div>
						</div>
					</div>
				</div>
				{{/each}}
			</div>
		</div>
	</section>
	<!--	EndSkills-->
	<section class="default-section pb-0" id="projects-section">
		<div class="container">
			<div class="row justify-content-center pb-5">
				<div class="col-md-12 heading-section text-center ftco-animate fadeInUp ftco-animated">
					<h1 class="big right-0">Projects</h1>
					<h2 class="mb-4 text-white">Our Projects</h2>
					<p>Far far away, behind the word mountains, far from the countries Vokalia and Consonantia</p>
				</div>
			</div>
			<div class="row">
				{{#each data.projects}}
					<div class="col-md-4">
						{{#if image}}
							<div
								class="project img ftco-animate d-flex justify-content-center align-items-center fadeInUp ftco-animated"
								style="background-image: url({{{cloudinaryUrl image}}});">
								<div class="overlay"></div>
								<div class="text text-center p-4">
									<h3><a href="">{{project.name}}</a></h3>
									<a href={{{project.url}}}><span>{{project.url}}</span></a>
								</div>
							</div>
						{{/if}}
					</div>
				{{/each}}
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!--	EndProjeect-->

	<!--	freelancing-->
	<section class="default-section ftco-hireme user-img margin-top"
			 style="background-image: url(images/cover-img.jpg)">
		<div class="container">
			<div class="row justify-content-center">
				<div class="col-md-7 ftco-animate text-center fadeInUp ftco-animated">
					<h2>I'm <span>Available</span> for freelancing</h2>
					<p>A small river named Duden flows by their place and supplies it with the necessary regelialia.</p>
					<p class="mb-0"><a href="" class="btn btn-primary py-3 px-5">Hire me</a></p>
				</div>
			</div>
		</div>
	</section>
	<!--	contactus-->
	<section class="default-section pb-0 contact-section ftco-no-pb" id="contact-section">
		<div class="container">
			<div class="row justify-content-center mb-5 pb-3">
				<div class="col-md-7 heading-section text-center ftco-animate fadeInUp ftco-animated">
					<h1 class="big right-0">Contact</h1>
					<h2 class="mb-4 text-white">Contact Me</h2>
					<p>Far far away, behind the word mountains, far from the countries Vokalia and Consonantia</p>
				</div>
			</div>
			<div class="row d-flex contact-info mb-5">
				<div class="col-md-6 col-lg-3 d-flex ftco-animate fadeInUp ftco-animated">
					<div class="align-self-stretch box p-4 text-center">
						<div class="icon d-flex align-items-center justify-content-center">
							<span class="icon-map-signs"></span>
						</div>
						<h3 class="mb-4">Address</h3>
						<p>{{data.employee.Address}}</p>
					</div>
				</div>
				<div class="col-md-6 col-lg-3 d-flex ftco-animate fadeInUp ftco-animated">
					<div class="align-self-stretch box p-4 text-center">
						<div class="icon d-flex align-items-center justify-content-center">
							<span class="icon-phone2"></span>
						</div>
						<h3 class="mb-4">Contact Number</h3>
						<p><a href="tel://1234567920">{{data.employee.mobile}}</a></p>
					</div>
				</div>
				<div class="col-md-6 col-lg-3 d-flex ftco-animate fadeInUp ftco-animated">
					<div class="align-self-stretch box p-4 text-center">
						<div class="icon d-flex align-items-center justify-content-center">
							<span class="icon-paper-plane"></span>
						</div>
						<h3 class="mb-4">Email Address</h3>
						<p><a href="mailto:info@yoursite.com">{{data.employee.Email}}</a></p>
					</div>
				</div>
				<div class="col-md-6 col-lg-3 d-flex ftco-animate fadeInUp ftco-animated">
					<div class="align-self-stretch box p-4 text-center">
						<div class="icon d-flex align-items-center justify-content-center">
							<span class="icon-globe"></span>
						</div>
						<h3 class="mb-4">Website</h3>
						<p><a href="">{{data.employee.website}}</a></p>
					</div>
				</div>
			</div>
			<div class="row no-gutters block-9">
				<div class="col-md-6 order-md-last d-flex">
					<form action="" class="bg-light p-4 p-md-5 contact-form">
						<div class="form-group">
							<input type="text" class="form-control" placeholder="Your Name">
						</div>
						<div class="form-group">
							<input type="text" class="form-control" placeholder="Your Email">
						</div>
						<div class="form-group">
							<input type="text" class="form-control" placeholder="Subject">
						</div>
						<div class="form-group">
							<textarea name="" id="" cols="30" rows="7" class="form-control"
									  placeholder="Message"></textarea>
						</div>
						<div class="form-group">
							<input type="submit" value="Send Message" class="btn btn-primary py-3 px-5">
						</div>
					</form>
				</div>
				<div class="col-md-6 d-flex">
					<div class="img" style="background-image: url({{{cloudinaryUrl data.image}}});"></div>
				</div>
			</div>
		</div>
	</section>
{{>footer}}
</div>
